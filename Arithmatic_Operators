#include <iostream >
using namespace std ;
int main () {
int  a = 22 ;
int   b = 88;
cout<< a <<endl << b << endl ;
cout <<"Addition " << a+b << endl ;
cout << "Subtraction" << a-b << endl ;
cout << "Multiplicaion" <<a*b << endl ;
cout << "Division " << a/b << endl ;
cout << "Modulo "<<  a%b << endl ;
return 0 ;
}
